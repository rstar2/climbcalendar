{
  "name": "climbcalendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo.svg",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:db": "firebase deploy --only firestore",
    "deploy:hosting": "firebase deploy --only hosting",
    "_predeploy:hosting": "firebase target:apply hosting main climbcalendar"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "@fullcalendar/multimonth": "^6.1.10",
    "@fullcalendar/react": "^6.1.10",
    "@tanstack/react-query": "^5.17.9",
    "@tanstack/react-query-devtools": "^5.17.9",
    "@tanstack/react-router": "^1.3.5",
    "@tanstack/router-devtools": "^1.3.5",
    "chakra-dayzed-datepicker": "^0.2.10",
    "chakra-react-select": "^4.7.6",
    "date-fns": "^3.2.0",
    "dayzed": "^3.2.3",
    "firebase": "^10.7.1",
    "formik": "^2.4.5",
    "framer-motion": "^10.17.9",
    "i18next": "^23.7.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "react-icons": "^5.0.1",
    "react-spinners": "^0.13.8",
    "react-use": "^17.4.2",
    "vite-plugin-pwa": "^0.17.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/gapi": "^0.0.47",
    "@types/gapi.client.calendar-v3": "^0.0.4",
    "@types/google.accounts": "^0.0.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vite-pwa/assets-generator": "^0.2.3",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
